{
  "name": "strcursor",
  "version": "0.1.1",
  "main": "./dist/js/index.js",
  "module": "./dist/js/index.js",
  "typings": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "CHANGELOG.md",
    "github",
    "README.md"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/ThatsEmbarrassing/string-cursor"
  },
  "description": "",
  "scripts": {
    "rollup:base-config": "rollup -c rollup.config.ts --configPlugin rollup-plugin-ts",
    "build:lib:development": "npm run rollup:base-config -- --environment mode:development",
    "build:lib:production": "npm run rollup:base-config -- --environment mode:production",
    "build": "npm run build:lib:production"
  },
  "keywords": [
    "string",
    "str",
    "string cursor",
    "string utils"
  ],
  "author": "",
  "license": "MIT",
  "exports": {
    ".": {
      "import": "./dist/js/index.js",
      "require": "./dist/js/index.js",
      "types": "./dist/types/index.d.ts",
      "default": "./dist/js/index.js"
    },
    "./operators": {
      "import": "./dist/js/operators.js",
      "require": "./dist/js/operators.js",
      "types": "./dist/types/operators.d.ts",
      "default": "./dist/js/operators.js"
    }
  },
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types/index.d.ts"
      ],
      "operators": [
        "./dist/types/operators.d.ts"
      ]
    }
  },
  "devDependencies": {
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.23.2",
    "@rollup/plugin-alias": "^5.0.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@tinkoff/utils": "^2.6.2",
    "@types/jest": "^29.5.5",
    "@types/ramda": "^0.29.3",
    "concurrently": "^8.2.1",
    "jest": "^29.7.0",
    "rollup": "^3.28.1",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^6.0.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "rollup-plugin-ts": "^3.4.4",
    "ts-essentials": "^9.3.2",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6",
    "utility-types": "^3.10.0"
  },
  "dependencies": {
    "@fullstacksjs/toolbox": "^4.1.3",
    "ramda": "^0.29.0"
  }
}
